package loops;

public class primeinnumber {

	public static void main(String[] args) {
		int n=143567;
		int last;
		int count=0;
		while(n>0)
		{
			last=n%10;
			for(int i=1;i<=last;i++)
			{
				count=0;
				if(last%i==0)
				{
					count++;
				}
				
			}
			if(count==2)
			{
				System.out.println(last);
			}
			n=n/10;
		}
	}

}
