package loops;

public class stringpalindrome {

	public static void main(String[] args) {
		String reverse="";
		String name="sreekanth";
		//System.out.println(name.length());
		//int na=name.length();
		for(int i=name.length()-1;i>=0;i--)
		{
			reverse=reverse+name.charAt(i);
			System.out.println(reverse);
		}
		

	}

}
