package flowcontro;

public class whileloop {

	public static void main(String[] args) {
		int i=1;
		int j=1;
		int k=1;
		while(i<=12)
		{
			System.out.println("hours================"+i);
			i++;
			
			while(j<=60)
			{
				System.out.println("minutes=========="+j);
				j++;
				while(k<=60)
				{
					System.out.println("seconds====="+k);
					k++;
				}

			}

	
		}


	}
}


