package flowcontro;

public class whileloop1 {

	public class whileloop2 {

	}

	public static void main(String[] args) {
		int i=1;
		int j=1;
		while(i<=10)
		{
		
			System.out.println("tables========="+i);
		
			
			while(j<=10)
			{
				System.out.println(i+"*"+j + "="+(i*j));
				j++;
			}
			i++;
			

	     }

}
}
